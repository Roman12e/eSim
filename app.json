{
  "expo": {
    "name": "The Best eSIM",
    "slug": "TheBesteSIM",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logo/Logo.png",
    "scheme": "thebestesim",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./logo/Logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./logo/Logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "server",
      "favicon": "./logo/Logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "94cb62ee-5723-4785-ab96-a8e456ffadd4"
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://thebestesim.expo.app"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./logo/Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.esim-app",
          "publishableKey": "pk_test_51SeyoGPH0upSs0gZtnGKU8km7ATlEt2CwcHj2OKpW4qjaNizetvKSrvGS30IS9vlAB33fzOqE5iahEmQH1UUqDVo005P756QM4"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}